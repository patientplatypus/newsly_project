{
  "AWSEBDockerrunVersion": 1,
  "Image": {
    "Name": "344677375614.dkr.ecr.us-west-2.amazonaws.com/newslyproduction",
    "Update": "true"
  },
  "volumes": [
    {
      "name": "newslyproduction",
      "host": {
        "sourcePath": "/newsly_project"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "newslyproduction",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 4000
        }
      ]
    }
  ]
}
